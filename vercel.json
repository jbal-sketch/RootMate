{
  "buildCommand": null,
  "outputDirectory": "marketing",
  "framework": null,
  "rewrites": [
    {
      "source": "/plant/:plantId",
      "destination": "/pages/plant.html?id=:plantId"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}

